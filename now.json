{
  "version": 2,
  "name": "dramatic-sound-board",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/sounds/.*",
      "dest": "/sounds/$1",
      "headers": { "cache-control": "maxage=31536000,immutable" }
    },
    {
      "handle": "filesystem"
    }
  ],
  "alias": [
    "dramatic-sound-board"
  ]
}